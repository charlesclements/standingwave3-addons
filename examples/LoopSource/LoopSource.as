package{		import com.SW3.util.Util;	import com.greensock.events.LoaderEvent;	import com.greensock.loading.LoaderMax;	import com.greensock.loading.MP3Loader;	import flash.display.Sprite;	import flash.media.Sound;	import flash.events.Event;	import com.greensock.TweenMax;	import flash.events.SampleDataEvent;	import com.demonsters.debugger.MonsterDebugger;	import com.noteflight.standingwave3.generators.SoundGenerator;	import com.noteflight.standingwave3.elements.AudioDescriptor;	import com.noteflight.standingwave3.output.AudioPlayer;	import com.noteflight.standingwave3.sources.LoopSource;		public class LoopSource extends Sprite	{				// Vars.		private var loader:LoaderMax;		//private var sound:Sound;						// Constructor.		public function LoopSource()		{						trace("LoopSource");						MonsterDebugger.initialize( this );						loader = new LoaderMax( { name:"audio", onComplete:onSoundsLoaded } );			loader.append( new MP3Loader( "assets/Beat.mp3", { autoPlay:false } ) );			loader.load();					}						private function onSoundsLoaded(e:LoaderEvent):void		{						trace("onSoundsLoaded");						// CDC - I still ahve not been able to make this LoopSource from StandingWave3 work properly.			// It keeps giving me : RangeError: Error #1506: The specified range is invalid.						var s:Sound = e.currentTarget.content[ 0 ] as Sound;			var gen:SoundGenerator = new SoundGenerator( s, new AudioDescriptor );			gen.fill( 100000 );			var loop:com.noteflight.standingwave3.sources.LoopSource = new com.noteflight.standingwave3.sources.LoopSource( gen.descriptor, gen );			loop.startFrame = 80;			loop.endFrame = 50000;			loop.firstFrame = 80;			var player:AudioPlayer = new AudioPlayer;			player.play( loop );					}					}		}